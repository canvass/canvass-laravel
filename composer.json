{
    "name": "canvass/laravel-service-provider",
    "license": "MIT",
    "description": "Laravel Service Provider for Canvass form builder",
    "authors": [
        {
            "name": "Michael Kelly",
            "email": "mkelly@newmediaretailer.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CanvassLaravel\\": "src/CanvassLaravel/"
        },
        "files": [
          "src/helpers.php"
        ],
        "classmap": [
            "database/migrations/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "CanvassLaravel\\Providers\\CanvassServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "laravel/framework": "^5.7",
        "fzaninotto/faker": "^1.8",
        "promo-code/laravel-schema-helper": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Canvass\\": "../../canvass-core/src/Canvass/"
        }
    }
}
