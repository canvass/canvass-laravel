{
    "name": "canvass/canvass-laravel",
    "license": "MIT",
    "description": "Laravel Service Provider for the Canvass form builder",
    "authors": [
        {
            "name": "Michael Kelly",
            "email": "mkelly@newmediaretailer.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CanvassLaravel\\": "src/CanvassLaravel/"
        },
        "files": [
          "src/helpers.php"
        ],
        "classmap": [
            "database/migrations/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "CanvassLaravel\\CanvassServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "canvass/canvass-paint-blade": "^1.02",
        "promo-code/laravel-schema-helper": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "laravel/framework": "^8.14",
        "laravel/legacy-factories": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/Tests/"
        }
    },
    "config": {
        "platform-check": false,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
